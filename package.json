{
  "name": "photo-editor",
  "version": "0.1.0",
  "description": "Simple and customizable photo editor for web applications.",
  "main": "./dist/photo-editor.js",
  "scripts": {
    "build": "npm run build:lib && npm run build:umd && npm run build:umd:min",
    "build:lib": "babel src --out-dir lib --ignore __tests__",
    "build:umd": "cross-env NODE_ENV=development webpack",
    "build:umd:min": "cross-env NODE_ENV=production webpack",
    "lint": "eslint src",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vtaits/photo-editor-js.git"
  },
  "keywords": [
    "editor",
    "image",
    "photo",
    "vanilla"
  ],
  "author": "Vadim Taits",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vtaits/photo-editor-js/issues"
  },
  "homepage": "https://github.com/vtaits/photo-editor-js#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "cross-env": "^5.0.1",
    "eslint": "^4.2.0",
    "eslint-plugin-jest": "^20.0.3",
    "jest": "^20.0.4",
    "path": "^0.12.7",
    "webpack": "^3.1.0"
  }
}
